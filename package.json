{
    "name": "export-typescript",
    "displayName": "export-typescript",
    "description": "Create an index.ts file and export",
    "version": "0.0.2",
    "publisher": "mscolnick",
    "engines": {
        "vscode": "^1.14.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mscolnick/export-typescript.git"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "tsx",
        "file",
        "template",
        "typescript",
        "react"
    ],
    "activationEvents": [
        "onCommand:extension.exportTypescript",
        "onLanguage:typescript"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.exportTypescript",
                "title": "Export Typescript"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "lint": "tslint --project tsconfig.json -t stylish",
        "lint-fix": "tslint --project tsconfig.json -t stylish --fix"
    },
    "devDependencies": {
        "@blueprintjs/tslint-config": "^1.3.1",
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^2.3.3",
        "tslint": "^5.10.0",
        "typescript": "^2.0.3",
        "vscode": "^1.1.4"
    },
    "dependencies": {}
}
