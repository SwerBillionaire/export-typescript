{
    "name": "export-typescript",
    "displayName": "export-typescript",
    "description": "Create an index.ts file and export",
    "version": "0.0.5",
    "publisher": "mscolnick",
    "engines": {
        "vscode": "^1.14.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mscolnick/export-typescript.git"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "tsx",
        "file",
        "template",
        "typescript",
        "react"
    ],
    "activationEvents": [
        "onCommand:extension.exportStarTypescript",
        "onCommand:extension.exportExportableDeclarationsTypescript",
        "onLanguage:typescript"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.exportStarTypescript",
                "title": "Export typescript - all declarations (as star)"
            },
            {
                "command": "extension.exportExportableDeclarationsTypescript",
                "title": "Export typescript - all exportable declarations"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "lint": "tslint --project tsconfig.json -t stylish",
        "lint-fix": "tslint --project tsconfig.json -t stylish --fix"
    },
    "devDependencies": {
        "@blueprintjs/tslint-config": "^3.0.2",
        "@types/mocha": "^8.0.3",
        "@types/node": "^14.6.0",
        "mocha": "^8.1.2",
        "tslint": "^6.1.3",
        "typescript": "^4.0.2",
        "vscode": "^1.1.37"
    },
    "dependencies": {
        "typescript-parser": "^2.6.1"
    }
}
