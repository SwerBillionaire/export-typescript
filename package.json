{
    "name": "export-typescript",
    "displayName": "export-typescript",
    "description": "Create index.ts file and export",
    "version": "0.0.1",
    "publisher": "mscolnick",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.exportTypescript"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.exportTypescript",
                "title": "Export Typescript"
            }
        ]
